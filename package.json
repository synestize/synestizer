{
  "name": "synestizer_slider",
  "version": "0.1.0",
  "description": "Listen to colours",
  "main": "src/index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "clean": "rm -rf js/*",
    "prewatch": "npm run clean",
    "watch": "webpack --progress --colors --watch --debug --output-pathinfo",
    "dev": "webpack-dev-server  --progress --colors --inline --hot",
    "prebuild": "npm run clean",
    "build": "NODE_ENV=production webpack -p --config webpack.config.js",
    "preserve": "npm run build",
    "serve": "static"
  },
  "repository": {
    "type": "git",
    "url": "github.com/synestize/synestizer.git"
  },
  "author": "Dan MacKinlay, Christoph Stähli, Kaspar König",
  "license": "GPL-3.0",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-system-import-transformer": "^2.4.0",
    "babel-plugin-transform-class-properties": "^6.16.0",
    "babel-plugin-transform-dead-code-elimination": "^2.2.1",
    "babel-plugin-transform-function-bind": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-escompress": "^1.0.0",
    "babel-preset-react": "^6.16.0",
    "babel-runtime": "^6.9.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "source-map-loader": "^0.1.5",
    "webpack": "^1.13.2"
  }
}
