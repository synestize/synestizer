{
  "name": "synestizer",
  "version": "0.2.0",
  "description": "Listen to colours",
  "main": "js-src/main.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "clean": "rm -rf js/*",
    "prewatch": "npm run clean",
    "watch": "webpack --progress --colors --watch --debug --output-pathinfo",
    "dev": "webpack-dev-server  --progress --colors",
    "build": "NODE_ENV=production webpack -p ",
    "prebuild": "npm run clean",
    "prepublish": "npm run build",
    "publish": "NODE_ENV=production rsync -av --delete css fonts js sound src index.html listentocolors:indigo_listentocolors_net"
  },
  "repository": {
    "type": "git",
    "url": "github.com/synestize/synestizer.git"
  },
  "author": "Dan MacKinlay, Christoph Stähli, Kaspar König",
  "license": "GPL-3.0",
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.9.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-uglify": "^1.0.2",
    "babel-preset-react": "^6.5.0",
    "babel-runtime": "^6.9.0",
    "babelify": "^7.2.0",
    "immutable": "^3.8.1",
    "ramda": "^0.19.1",
    "react": "0.14.8",
    "react-addons-pure-render-mixin": "0.14.8",
    "react-addons-shallow-compare": "0.14.8",
    "react-addons-update": "0.14.8",
    "react-dom": "0.14.8",
    "rx": "^4.1.0",
    "source-map-loader": "^0.1.5",
    "webpack": "^1.13.1",
    "webrtc-adapter": "^1.2.2",
    "worker-loader": "^0.7.0"
  }
