    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-plugin-uglify": "^1.0.2",
    "babel-preset-react": "^6.3.13",
    "babelify": "^7.2.0",
    "immutable": "^3.7.6",
    "ramda": "^0.19.1",
    "react": "^0.14.6",
    "react-addons-update": "^0.14.7",
    "react-dom": "^0.14.6",
    "react-redux": "^4.4.1",
    "redux": "^3.3.1",
    "redux-core": "^0.1.1",
    "rx": "^4.0.7",
    "source-map-loader": "^0.1.5",
