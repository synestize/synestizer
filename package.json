{
  "name": "synestizer",
  "version": "1.0.0",
  "description": "Listen to colours",
  "main": "src/index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "clean": "rm -rf js/*",
    "prewatch": "npm run clean",
    "watch": "webpack --progress --colors --watch --debug --output-pathinfo",
    "dev": "webpack-dev-server  --progress --colors --inline --hot",
    "build": "NODE_ENV=production webpack -p ",
    "prebuild": "npm run clean",
    "prepublish": "npm run build",
    "publish": "NODE_ENV=production rsync -av --delete css fonts js sound index.html listentocolors:violet_listentocolors_net",
    "preserve": "build",
    "serve": "static"
  },
  "repository": {
    "type": "git",
    "url": "github.com/synestize/synestizer.git"
  },
  "author": "Dan MacKinlay, Christoph Stähli, Kaspar König",
  "license": "GPL-3.0",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-system-import-transformer": "^2.3.0",
    "babel-plugin-transform-dead-code-elimination": "^2.2.0",
    "babel-plugin-transform-function-bind": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-runtime": "^6.9.0",
    "hacktimer": "^1.1.0",
    "localforage": "^1.4.2",
    "node-static": "^0.7.8",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "redux-core": "^0.1.1",
    "redux-logger": "^2.6.1",
    "redux-persist": "^3.5.0",
    "redux-thunk": "^2.1.0",
    "rxjs": "^5.0.0-beta.12",
    "source-map-loader": "^0.1.5",
    "tone": "^0.7.1",
    "webpack": "^1.13.2",
    "webrtc-adapter": "^1.4.0",
    "worker-loader": "^0.7.1"
  }
}
