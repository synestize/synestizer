{
  "name": "synestizer",
  "version": "1.0.0",
  "description": "Listen to colours",
  "main": "js-src/main.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "clean": "rm -rf js/*",
    "prewatch": "npm run clean",
    "watch": "webpack --progress --colors --watch --debug --output-pathinfo",
    "dev": "webpack-dev-server  --progress --colors",
    "build": "NODE_ENV=production webpack -p ",
    "prebuild": "npm run clean",
    "prepublish": "npm run build",
    "publish": "NODE_ENV=production rsync -av --delete css fonts js sound index.html listentocolors:indigo_listentocolors_net"
  },
  "repository": {
    "type": "git",
    "url": "github.com/synestize/synestizer.git"
  },
